        <div id="{{ col.nomBloccol }}" class="{{ col.classBloccol }} col-xs-12 {{ colsm }} box-1" style="float:left; {{ width }} margin-bottom:20px;">
				               <div class="title-box">
				                  <div class="inner-box inner-box-1">
				                     <h2>{{ col.htmlBloccol|extracthtml('titre') }}</h2>
				                     <span class="col-1">{{ col.htmlBloccol|extracthtml('date') }}
									 {% if col.htmlBloccol|extracthtml('client') != "" %}
									 &nbsp;&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;&nbsp;Client: {{ col.htmlBloccol|extracthtml('client') }}</span>
									{%endif%}
				                  </div>
				               </div>
				            <div class="content-box-1">
				               <div class="wrapper-extra visuel">
							   		
									{% set texte = col.htmlBloccol|extracthtml('page') %}
						  			{{ texte|raw }}	
									{% if col.htmlBloccol|extracthtml('liensite') != "" %}
				                    <br><br><a href="{{ col.htmlBloccol|extracthtml('liensite') }}" target="_blank" class="button">Visiter le site</a>
				                     {% endif %}
				               </div>
							   </div>
		</div>
